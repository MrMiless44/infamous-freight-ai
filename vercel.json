{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://infamous-freight-api.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "Inf√¶mous Freight",
    "NEXT_PUBLIC_ENV": "production"
  }
}
